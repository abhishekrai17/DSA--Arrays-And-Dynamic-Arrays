#include <iostream>
using namespace std;
int main() {
    // your code goes here
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int newarray[n];
    for(int i=0;i<n;i++){
        newarray[arr[i]]=i;
    }
    for(int i=0;i<n;i++){
        cout<<newarray[i]<<" ";
    }
    return 0;
}
